{
  "expo": {
    "name": "loop",
    "slug": "loop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Good.png",
    "scheme": "loop",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.kreakxx.loop",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Good.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "useNextNotificationsApi": true,
      "permissions": ["NOTIFICATIONS", "CAMERA", "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Good.png",
          "imageWidth": 200,
          "backgroundColor": "#ffffff"
        }
      ]

    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "336cc2c1-a7ab-49f0-b38f-32d2517737bf"
      }
    }
  }
}
